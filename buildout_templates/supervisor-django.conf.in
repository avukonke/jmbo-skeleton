[program:${name}.django]
numprocs=${num-processes}
numprocs_start=1
process_name=%(program_name)s_%(process_num)s
command=${buildout:directory}/../python/bin/gunicorn ${control-script}-wsgi:application --max-requests=${max-requests} -b ${host}:${port-prefix}%(process_num)s
directory=${buildout:bin-directory}
user=${user}
stdout_logfile=${log}
stderr_logfile=${error-log}
stopsignal=QUIT
environment=PYTHON_EGG_CACHE='/var/www/.python-eggs'
